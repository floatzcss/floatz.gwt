<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
		"http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Demo">

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='com.floatzcss.demo.client.Demo'/>

	<!-- Other module inherits                                -->
	<inherits name='com.floatzcss.gwt.Css'/>
	<!--<script src="floatz-1.3.0/scripts/jquery-1.11.2.min.js"/>
	<script src="floatz-1.3.0/scripts/ua-parser-0.7.3.min"/>
	<script src="floatz-1.3.0/scripts/floatz.js"/>
	<script src="floatz-1.3.0/scripts/floatz.mobile.js"/>
	<script src="floatz-1.3.0/scripts/floatz.skiplink.js"/>-->

	<!-- Specify the app servlets.                   -->
	<servlet path='/DemoService' class='com.floatzcss.demo.server.DemoServiceImpl'/>

</module>